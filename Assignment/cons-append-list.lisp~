(defun sample(first_lst,second_lst,a)
  (write ("Trying cons: ")
  (cons (a,first_lst))
  (cons (first_lst,second_lst))
  (write ("Trying append: ")
  (append (a,first_lst))
  (append (first_lst,a))
  (append (first_lst,second_lst))
  (write ("Trying list: ")
  (list (a,first_lst))
  (list(first_lst,second_lst))
)
